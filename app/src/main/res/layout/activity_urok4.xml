<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".Urok4">

    <TextView
        android:id="@+id/tvText_Urok4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="TextView"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageStatic"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvText_Urok4"
        app:srcCompat="@drawable/kompas1" />

    <ImageView
        android:id="@+id/imageDinamic"
        android:layout_width="390dp"
        android:layout_height="390dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageStatic"
        app:layout_constraintEnd_toEndOf="@+id/imageStatic"
        app:layout_constraintStart_toStartOf="@+id/imageStatic"
        app:layout_constraintTop_toTopOf="@+id/imageStatic"
        app:srcCompat="@drawable/kompas2" />
</androidx.constraintlayout.widget.ConstraintLayout>